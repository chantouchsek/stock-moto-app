{
    "name": "com.moto.stock.system",
    "version": "1.0.0",
    "description": "Brand new cordova project with framework7 - vue - webpack!",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "browser": "npm run build && cordova run browser",
        "android": "npm run build && cordova run android",
        "ios": "npm run build && cordova run ios"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "license": "ISC",
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "cheerio": "^1.0.0-rc.2",
        "chokidar": "^2.0.4",
        "clean-webpack-plugin": "^1.0.0",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "current-device": "^0.7.8",
        "epipebomb": "^1.0.0",
        "extract-text-webpack-plugin": "next",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.10.0",
        "path": "^0.12.7",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "svg-url-loader": "^2.3.2",
        "uglify-js": "^3.4.9",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "url-loader": "^1.1.2",
        "vue-hot-reload-api": "^2.3.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^15.4.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.21",
        "webpack": "^4.26.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    },
    "dependencies": {
        "axios": "^0.19.0-beta.1",
        "cordova-android": "7.1.4",
        "cordova-browser": "5.0.4",
        "cordova-ios": "4.5.5",
        "cordova-plugin-camera": "^4.0.3",
        "cordova-plugin-ios-camera-permissions": "1.2.0",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "font-awesome": "^4.7.0",
        "framework7": "^3.6.2",
        "framework7-icons": "^2.1.0",
        "framework7-vue": "^3.6.2",
        "lodash.debounce": "^4.0.8",
        "loglevel": "^1.6.1",
        "material-design-icons": "^3.0.1",
        "vue": "^2.5.21",
        "vuex": "^3.0.1"
    },
    "displayName": "MotoStockManagementSystem",
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-camera": {},
            "cordova-plugin-ios-camera-permissions": {
                "CAMERA_USAGE_DESCRIPTION": "This app needs camera access",
                "MICROPHONE_USAGE_DESCRIPTION": "This app needs microphone access",
                "PHOTOLIBRARY_ADD_USAGE_DESCRIPTION": "This app needs write-access to photo library",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": "This app needs read/write-access photo library access"
            }
        },
        "platforms": [
            "android",
            "ios",
            "browser"
        ]
    }
}